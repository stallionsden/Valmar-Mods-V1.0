<Valmar>	
	<append xpath="/blocks">
	 <!-- Class Mod -->
    <block name="Class - Hunter (Locked)">
        <property name="Group" value="Class System" />
        <property name="CustomIcon" value="Class - Hunter" />
		<property name="DescriptionKey" value="Class - Hunter (Locked)Desc"/>
        <!--        <property name="CustomIcon" value="suitcase" /> -->
        <property name="Material" value="Mcloth" />
       <property name="Shape" value="ModelEntity"/>
        <property name="Model" value="@:Entities/LootContainers/luggageMediumClosedPrefab.prefab"/>
        <property name="Place" value="TowardsPlacer" />
        <drop event="Destroy" count="0" prob="1" />
        <property name="CanPickup" value="true" />
        <property name="DowngradeBlock" value="Class - Hunter" />
        <property class="UpgradeBlock">
            <property name="ToBlock" value="Class - Hunter" />
            <property name="Item" value="classKey" />
            <property name="ItemCount" value="1" />
            <property name="UpgradeHitCount" value="1" />
        </property>
    </block>
	
    <block name="Class - Hunter">
        <property name="Class" value="Loot" />
        <property name="Group" value="Survival" />
		<property name="DescriptionKey" value="Class - HunterDesc"/>
		 <property name="Material" value="Mcloth" />
        <!--        <property name="CustomIcon" value="suitcase" /> -->
       <property name="Shape" value="ModelEntity"/>
        <property name="Model" value="@:Entities/LootContainers/luggageMediumClosedPrefab.prefab"/>
        <property name="Place" value="TowardsPlacer" />
        <property name="LootList" value="hunter" />
        <drop event="Destroy" count="0" />
        <drop event="Fall" name="woodDebris" count="1" prob="1.0"  stick_chance=".75" />
        <property name="LPHardnessScale" value="8" />
    </block>
